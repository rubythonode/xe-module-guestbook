<query id="getGuestbookItemCount" action="select">
    <tables>
        <table name="guestbook_item" />
    </tables>
    <columns>
        <column name="count(*)" alias="count" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
